{
  "version": 2,
  "builds": [
    {
      "src": "server-vercel-upstash.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server-vercel-upstash.js"
    }
  ],
  "functions": {
    "server-vercel-upstash.js": {
      "maxDuration": 30
    }
  }
} 